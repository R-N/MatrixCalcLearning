@article{Dirgantara2017,
abstract = {Learning media stimulate and support the students in learning process, especially in matrix cases. Many students poses the difficulties in learning due to the usage of non-interactive learning media, such as books or lecturer notes. This study aims to build a web- based matrix operations application that support the students learning process as a learning media. This learning media is built using prototyping model process. The process is divided into four main stages, namely communication, quick plan, quick design, construction, and deployment. The result of this study is a web-based matrix operations application that has basic features of addition, subtraction, multiplication, determinant, and transpose matrix. Index},
author = {Dirgantara, Harya Bima and Marselino, Tedi Lesmana},
file = {:C$\backslash$:/Users/MojoMacW7/Documents/NetBeansProjects/Matrices/paper/ref/631-ArticleText-1666-1-10-20180402.pdf:pdf},
journal = {IJNMT},
keywords = {learning media,matrix,prototype model,web-based},
mendeley-tags = {learning media,matrix,prototype model,web-based},
number = {December},
pages = {105--111},
title = {{Development of Web-based Matrix Operations Calculation as a Learning Media}},
volume = {IV},
year = {2017}
}
